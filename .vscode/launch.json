    {
      "name": "Launch demo:client (edge)",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://localhost:3003/",
      "webRoot": "${workspaceFolder}/apps/demo/src",
      "userDataDir": "${workspaceFolder}/.out/edge/apps",
      "internalConsoleOptions": "openOnSessionStart",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/apps/demo",
      "smartStep": true,
      "preLaunchTask": "Debug server",
      "presentation": {
        "hidden": false,
        "group": "3",
        "order": 2
      }
    },
    {
      "name": "Launch demo:client (chrome)",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:3003/",
      "webRoot": "${workspaceFolder}/apps/demo/src",
      "userDataDir": "${workspaceFolder}/.out/chrome/apps",
      "internalConsoleOptions": "openOnSessionStart",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}/apps/demo",
      "smartStep": true,
      "presentation": {
        "hidden": false,
        "group": "3",
        "order": 2
      }
    },
    {
      "type": "bun",
      "request": "attach",
      "name": "Attach to Bun",
      "url": "ws://localhost:6499/"
    }
  ],
  "compounds": [
    {
      "name": "Debug demo (edge)",
      "configurations": ["Launch demo:client (edge)", "Attach to Bun"],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "1",
        "order": 1
      }
    },
    {
      "name": "Debug demo (chrome)",
      "configurations": ["Launch api", "Launch demo:serve", "Launch demo:client (chrome)"],
      "stopAll": true,
      "presentation": {
        "hidden": false,
        "group": "1",
        "order": 1
      }
    }
  ]
}
