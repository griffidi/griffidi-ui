{
  "name": "@gui/components",
  "private": false,
  "version": "1.0.0",
  "packageManager": "pnpm@10.9.0",
  "engines": {
    "node": ">=23.11.0"
  },
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    "./button/button.tsx": {
      "import": "./src/button/button.tsx"
    },
    "./button/link-button.tsx": {
      "import": "./src/button/link-button.tsx"
    },
    "./card/*.tsx": {
      "import": "./src/card/*.tsx"
    },
    "./progress-bard/*.tsx": {
      "import": "./src/progress-bar/*.tsx"
    },
    "./progress-spinner/*.tsx": {
      "import": "./src/progress-spinner/*.tsx"
    },
    "./table/*.tsx": {
      "import": "./src/table/*.tsx"
    },
    ".": {
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "task build",
    "build": "task build"
  },
  "dependencies": {
    "@griffel/react": "^1.5.29",
    "@gui/core": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  }
}
